{
    "name": "personal-galaxy/calendar",
    "type": "library",
    "description": "Calendar model",
    "keywords": ["calendar"],
    "homepage": "http://github.com/PersonalGalaxy/Calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/PersonalGalaxy/Calendar/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/immutable": "^2.8",
        "innmind/event-bus": "^2.1",
        "innmind/time-continuum": "^1.1",
        "innmind/specification": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PersonalGalaxy\\Calendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PersonalGalaxy\\Calendar\\": "tests/",
            "Fixture\\PersonalGalaxy\\Calendar\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "giorgiosironi/eris": "^0.10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
